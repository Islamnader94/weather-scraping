#!/Users/islamnader94/Documents/Personal-Work/scraping/scrapy/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'bCNC==0.9.14.301','console_scripts','bCNC'
__requires__ = 'bCNC==0.9.14.301'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('bCNC==0.9.14.301', 'console_scripts', 'bCNC')()
    )
